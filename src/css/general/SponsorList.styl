
.general--sponsorlist {
    display: block
    padding: 32px;
    ul {
      margin: 0
      padding: 0
      list-style: none
      margin-bottom: 15px;
    }
    .title {
        color: #FFD200;
        font-size: 1.5em;
    }
    .sponsor {
        margin: 15px 0;
          img {
              border: 1px solid #eee;
              display: block;
              margin: 0 auto;
              max-width: 100%
          }
    }
    @media only screen and (min-width: 600px) {
      ul {
        margin: 0 -1em;
      }
      .sponsor {
        display: inline-block;
        box-sizing: border-box;
        max-width: 50%;
        padding: 0 1em;
      }
    }
    @media only screen and (min-width: 1024px) {
      .sponsor {
        display: inline-block;
        max-width: 25%;
      }
    }
}
